<a href="{{ path('models_edit', {'id': model.id}) }}" class="btn btn-sm btn-primary">Edit</a>
<form action="{{ path('models_delete', {'id': model.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');" method="post" style="display:inline-block;">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ model.id) }}">
    <button class="btn btn-sm btn-danger" >Delete</button>
</form>